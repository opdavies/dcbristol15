
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="/favicon.ico">
    <title>{{ site.title }}</title>

    <link href="{{ theme_path('assets/css/bootstrap.min.css') }}" rel="stylesheet">
    <link href="{{ theme_path('assets/css/main.css') }}" rel="stylesheet">
    <link href="{{ theme_path('assets/css/animate.css') }}" rel="stylesheet">

    <!--[if lt IE 9]><script src="/js/ie8-responsive-file-warning.js"></script><![endif]-->
    <link href='https://fonts.googleapis.com/css?family=Raleway' rel='stylesheet' type='text/css'>

    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

{% if site.google_analytics_tracking_id %}
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', '{{ site.google_analytics_tracking_id }}', 'auto');
        ga('send', 'pageview');

    </script>
{% endif %}
</head>
<body>



{% block content_wrapper %}
  {% block content %}{% endblock %}
{% endblock %}


<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
<script src="/js/bootstrap.min.js"></script>
<script src="/js/jquery.nicescroll.min.js"></script>
<script>
    $(function() {$("html").niceScroll({cursorwidth: "12",cursorcolor: "#344F73"});});

    $('#mc-embedded-subscribe').click(function() {
        ga('send', {
            hitType: 'event',          // Required.
            eventCategory: 'form',   // Required.
            eventAction: 'click',      // Required.
            eventLabel: 'Interest Form',
        });
    });

    $('.twitter-link').click(function() {
        ga('send', {
            hitType: 'event',          // Required.
            eventCategory: 'link',   // Required.
            eventAction: 'click',      // Required.
            eventLabel: 'Twitter',
        });
    });

</script>
</body>
</html>